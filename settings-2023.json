    //"editor.fontFamily": "'Fira Code', 'Courier New', monospace",
    //"editor.fontLigatures": true,
    "artisan.docker.enabled": true,
    "artisan.docker.command": "docker-compose exec -T php74apache",
    "todohighlight.keywords": [
        "DEBUG:",
        "REVIEW:",
        {
            "text": "NOTE:",
            "color": "#fff",
            "backgroundColor": "pink",
            "overviewRulerColor": "pink"
        },
        {
            "text": "HACK:",
            "color": "#000",
            "isWholeLine": false,
        },
    ],
    "todo-tree.filtering.excludeGlobs": [
        "**/credimarket/**",
        "**/vendor/**",
        "**/node/node_modules/**",
        "**/node_modules/**",
        "**/dist/**",
        "**/resources/**",
        "**/public/**",
        "**/core/**",
        "**/common-libs/**",
        "**/openlead/**",
        "**/appuser-frontend/**",
        "**/bkmdoc-frontend/**",
        "**/crm-frontend/**",
        //"**/bkmdocapi/**",
        "**/laradock/**",
    ],
    "workbench.colorCustomizations": {
        "editorCursor.foreground": "#ff0000",
        "terminalCursor.foreground": "#ff0000"
    },
    "todo-tree.customHighlight": {
        "NOTE": {
            "icon": "check",
            "type": "line"
        },
        "FIXME": {
            "foreground": "black",
            "iconColour": "yellow"
        }
    },
    "yaml.schemas": {
        "file:///home/juan/.vscode/extensions/docsmsft.docs-yaml-0.2.3/schemas/toc.schema.json": "/toc\\.yml/i"
    },
    "editor.cursorBlinking": "expand",
    "editor.cursorWidth": 4,
    //"artisan.docker.enabled": true,
    //"artisan.docker.command": "docker exec -to workdir php56",
    "[yaml]":  {
		"editor.insertSpaces": true,
		"editor.tabSize": 2,
		"editor.autoIndent": "none"
    },
    "todo-tree.highlights.customHighlight": {
        "NOTE": {
            "icon": "check",
            "type": "line"
        },
        "FIXME": {
            "foreground": "black",
            "iconColour": "yellow"
        }
    },
    "workbench.iconTheme": "material-icon-theme",
    "terminal.integrated.fontFamily": "monospace",
    "redhat.telemetry.enabled": false,
    "code-runner.executorMap": {
        "javascript": "node",
        "php": "/usr/local/bin/php",
        "python": "python",
        "html": "/usr/bin/google-chrome-stable /usr/share/man/man1/google-chrome-stable.1.gz",
    },
    "todo-tree.general.tags": [
        "BUG",
        "HACK",
        "FIXME",
        "TODO",
        "XXX",
        "[ ]",
        "[x]"
    ],
    "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
    "sync.gist": "347e21fde7d4e0191adad6a2144c29a0",
    //"workbench.colorTheme": "Winter is Coming (Dark Black)",
    // PHP Code Sniffer
    // PHP 
    "[php]": {
        "editor.tabSize": 4,
        "editor.defaultFormatter": "bmewburn.vscode-intelephense-client",
        "editor.formatOnSave": true
    },
    "code-runner.languageIdToFileExtensionMap": {
        "bat": ".bat",
        "powershell": ".ps1",
        "typescript": ".ts"
    },
    "command-runner.terminal.name": "runCommand",
    "command-runner.terminal.autoClear": true,
    "command-runner.terminal.autoFocus": true,
    "command-runner.commands": {

        "echo": "echo 'this is a test command: ${workspaceFolder}'"
    },
    "workspaceCacheClean.autoReload": true,
    "markdown-preview-enhanced.previewTheme": "atom-light.css",
    "blade.format.enable": true,
    "json.schemas": [
        {
            "fileMatch": [
                "pull_request.yaml",
                "build_docker_image.yaml",
                "deploy_k8s_preprod",
                "deploy_k8s_prod"
            ],
            "url": "http://json.schemastore.org/github-workflow"
        }
    ],
    "intelephense.environment.phpVersion": "8.2.8",
    "explorer.openEditors.sortOrder": "alphabetical",
    "namespaceResolver.sortOnSave": true,
    "namespaceResolver.sortAlphabetically": true,
    "editor.fontSize": 19,
    "editor.tabSize": 5,
    "sonarlint.rules": {
        "php:S1808": {
            "level": "off"
        }
    },
}
