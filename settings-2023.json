    //"better-phpunit.docker.enable": true,
    //"better-phpunit.suiteSuffix": "",
    //"better-phpunit.docker.command": "docker exec -ti php73",
    //"better-phpunit.docker.paths": {
      //  "/datos/web/bkmdocapi": "/datos/web/bkmdocapi"
    //},
    //"better-phpunit.commandSuffix": "-c /datos/web/bkmdocapi/phpunit.xml",
    "better-phpunit.docker.enable": true,
    "better-phpunit.docker.command": "docker exec -ti php74apache",
    "better-phpunit.docker.paths": {
        "/datos/web/crm-business": "/datos/web/crm-business",
        "/datos/web/crm-clients": "/datos/web/crm-clients",
        "/datos/web/crm-externals": "/datos/web/crm-externals",
        "/datos/web/crm-communications": "/datos/web/crm-communications",
        "/datos/web/crm-logs": "/datos/web/crm-logs",
        "/datos/insurance/web/projects/tarificar" : "/var/tarificar"
    },
    "better-phpunit.phpunitBinary": "./var/tarificar/vendor/bin/phpunit",
    "better-phpunit.commandSuffix": "-c /var/tarificar/phpunit.xml --testdox --coverage-text",
    "better-phpunit.suiteSuffix": "-c /var/tarificar/phpunit.xml --testdox --coverage-text",
    //"better-phpunit.suiteSuffix": "-c /datos/web/crm-externals/phpunit.xml --testdox --coverage-text",
    "editor.bracketPairColorization.enabled": true,
    "editor.guides.bracketPairs":"active",
    "files.associations": {
        "*.module": "php",
        "*.php": "php",
        "*.blade.php": "blade",
        "*.tpl": "html"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    "[json]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "[javascript]": {
        "editor.tabSize": 2
    },
    "[typescript]": {
        "editor.tabSize": 2
    }, // if you would like to enable blade format
    "[blade]": {
        //"editor.defaultFormatter": "shufo.vscode-blade-formatter",
        "editor.formatOnSave": true,
        "editor.autoClosingBrackets": "always"
    },
    "emmet.triggerExpansionOnTab": true, // enable tab to expanse emmet tags
    "emmet.includeLanguages": {
        "[blade]":"html"
    },
    "beautify.language": {
        "html": [
            "blade",
            "html"
        ]
    },
    // PHP - Validate: Executable path (ubicación del intérprete de PHP)
    "php.validate.executablePath": "/usr/local/bin/php",
    // PHP: Validate: Run (la validación se ejecuta al guardar o al escribir)
    "php.validate.run": "onType",
    // Basic PHP code suggestions we want to disable because these 
    // suggestions are polluted by the default PHP suggestions.
    "php.suggest.basic": false,
    // PHP: Executable Path (Ubicación del intérprete de PHP)
    "php.executablePath": "/usr/local/bin/php",
    // To enable suggestions for Symfony test classes, 
    // all you need to do is edit your settings.json file and add:
    "intelephense.files.exclude": [
        "**/.git/**",
        "**/.svn/**",
        "**/.hg/**",
        "**/CVS/**",
        "**/.DS_Store/**",
        "**/node_modules/**",
        "**/bower_components/**",
        "**/vendor/**/{Test,test,Tests,tests}/**/*Test.php",
        "**/resources/**"
    ],
